sudo dnf config-manager --add-repo=https://dl.rockylinux.org/pub/rocky/8/BaseOS/x86_64/os/ && \
sudo dnf config-manager --add-repo=https://dl.rockylinux.org/pub/rocky/8/AppStream/x86_64/os/ && \
sudo dnf config-manager --add-repo=https://dl.fedoraproject.org/pub/epel/epel-release-latest-8.noarch.rpm


